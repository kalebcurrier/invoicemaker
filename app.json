{
  \"expo\": {
    \"name\": \"InvoiceMaker\",
    \"slug\": \"invoicemaker\",
    \"scheme\": \"invoicemaker\",
    \"version\": \"1.0.0\",
    \"orientation\": \"portrait\",
    \"icon\": \"./assets/icon.png\",
    \"splash\": {
      \"image\": \"./assets/splash.png\",
      \"resizeMode\": \"contain\",
      \"backgroundColor\": \"#ffffff\"
    },
    \"userInterfaceStyle\": \"automatic\",
    \"plugins\": [\"expo-router\"],
    \"assetBundlePatterns\": [\"**/*\"],
    \"ios\": {
      \"supportsTablet\": true
    },
    \"android\": {
      \"adaptiveIcon\": {
        \"foregroundImage\": \"./assets/adaptive-icon.png\",
        \"backgroundColor\": \"#ffffff\"
      }
    },
    \"web\": {
      \"bundler\": \"metro\",
      \"output\": \"static\"
    }
  }
}
